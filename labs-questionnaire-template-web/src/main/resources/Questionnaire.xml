<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<questionnaire-forms
        xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
        xsi:noNamespaceSchemaLocation='XSDforWizard.xsd'>
    <questionnaire-form form-id="f1">
        <form-name>Test template</form-name>
        <pages>
            <page page-id="p1" page-number="1">
                <page-name>First page</page-name>
                <groups-of-questions>
                    <group group-id="g11">
                        <group-name>First page</group-name>
                        <questions>
                            <question answer-required="false" question-id="q111">
                                <question-title>How you're doing? :)</question-title>
                                <question-type>TEXT</question-type>
                                <rules>
                                    <rule>
                                        <parentId> </parentId>
                                        <method> </method>
                                    </rule>
                                </rules>
                                <help-text>answer "good"!</help-text>
                                <options>
                                    <option> </option>
                                </options>
                                <range/>
                                <grid>
                                    <columns>
                                        <column> </column>
                                    </columns>
                                    <rows>
                                        <row> </row>
                                    </rows>
                                </grid>
                                <default-answers>
                                    <default-answer>Good</default-answer>
                                </default-answers>
                            </question>
                        </questions>
                    </group>
                </groups-of-questions>
            </page>
        </pages>
    </questionnaire-form>
</questionnaire-forms>
