<?xml version="1.0" encoding="UTF-8"?>
<questionnaire-forms>
	<questionnaire-form>
		<form-name></form-name>
		<pages>
			<page>
				<page-number></page-number>
				<groups-of-questions>
					<group>
						<group-name></group-name>
						<questions>
							<question required-answer="required" question-id="2">
								<question-title></question-title>
									<question-type></question-type>
									<help-text></help-text>
									<dependent-questions>
										<dependent-question>
											<question-id></question-id>
											<required-answer></required-answer>
										</dependent-question>
									</dependent-questions>
									
									<multiple-choice-options>
										<option></option>
									</multiple-choice-options>
									
									<checkboxes-options>
										<option></option>
									</checkboxes-options>

									<choose-from-list-options>
										<option></option>
									</choose-from-list-options>

									<range range-begin="0" range-end="1"></range>
								
									<grid>
										<columns>
											<column></column>
										</columns>
										<rows>
											<row></row>
										</rows>
									</grid>                          
							</question>
						</questions>
					</group>
				</groups-of-questions>
			</page>
		</pages>
	</questionnaire-form>
</questionnaire-forms>
